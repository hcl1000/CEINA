#ifndef _ACTIONS_
#define _ACTIONS_


#define PROCESSENTRY(i) \
action processentry##i##() { \
    write_data_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

#define NOEQU0_PROCESSENTRY(i) \
action noequ0_processentry##i##() { \
    noequ0_write_data_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

#define DO_CLEANENTRY(i) \
action do_cleanEntry##i##() { \
    clean_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

#define PROCESSENTRY_AND_WRITE_TO_PACKET(i,j) \
action processentry##i##andWriteToPacket() { \
     hdr.p4ml_entries.data##j## = write_read_data_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

#define NOEQU0_PROCESSENTRY_AND_WRITE_TO_PACKET(i,j) \
action noequ0_processentry##i##andWriteToPacket() { \
    hdr.p4ml_entries.data##j## = noequ0_write_read_data_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

#define ENTRY_WRITE_TO_PACKET(i,j) \
action entry##i##WriteToPacket() { \
    hdr.p4ml_entries.data##j## = read_data_entry##i##.execute(hdr.p4ml_agtr_index.agtr); \
} \

